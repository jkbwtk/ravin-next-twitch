    "dev": "cross-env DEV=true TS_NODE_PROJECT=./backend/tsconfig.json tsnd -r tsconfig-paths/register ./backend/src/index.ts",
    "playground": "cross-env DEV=true TS_NODE_PROJECT=./backend/tsconfig.json tsnd -r tsconfig-paths/register ./backend/src/playground.ts",
