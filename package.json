{
  "name": "ravin-next-twitch",
  "version": "0.0.1",
  "description": "Experimental twitch bot with dashboard",
  "main": "server/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node --no-node-snapshot .",
    "dev": "cross-env DEV=true VITE_CJS_IGNORE_WARNING=true TS_NODE_PROJECT=./backend/tsconfig.json tsnd -r tsconfig-paths/register --no-node-snapshot ./backend/src/index.ts",
    "playground": "cross-env DEV=true VITE_CJS_IGNORE_WARNING=true TS_NODE_PROJECT=./backend/tsconfig.json tsnd -r tsconfig-paths/register --no-node-snapshot ./backend/src/playground.ts",
    "build": "vite build && cd ./backend && tsc -b",
    "watch": "cd backend && tsc -b --watch",
    "lint": "eslint --fix --ext .ts,.tsx ./frontend/src ./backend/src ./shared/src",
    "test": "vitest"
  },
  "author": "jkbwtk",
  "license": "ISC",
  "homepage": "https://github.com/jkbwtk/ravin-next-twitch#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkbwtk/ravin-next-twitch.git"
  },
  "_moduleAliases": {
    "#shared": "server/",
    "#types": "server/types",
    "#lib": "server/lib",
    "#database": "server/database",
    "#bot": "server/bot",
    "#server": "server/server",
    "#routers": "server/server/routers",
    "#jobs": "server/jobs"
  },
